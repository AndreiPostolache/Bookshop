{
	"_version": "1.29.0",
	"sap.app": {
		"id": "bookshop",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				"de",
				"it",
				"fr",
				"en"
			]
			
		},
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{AppTitle}}",
		"description": "{{AppSubtitle}}",
		"dataSources": {
			"mainService": {
				"uri": "odata/v4/catalog/",
				"type": "OData",
				"settings": {
					"odataVersion": "4.0"
				}
			}
		},
		"crossNavigation": {
			"inbounds": {
				"Books-display": {
					"signature": {
						"parameters": {},
						"additionalParameters": "allowed"
					},
					"semanticObject": "Books",
					"action": "display",
					"title": "{{AppTitle}}",
					"subTitle": "{{AppSubtitle}}",
					"icon": ""
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"dependencies": {
			"minUI5Version": "1.102.1",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.fe.templates": {},
				"sap.ui.integration": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleUrl": "i18n/i18n.properties",
					"supportedLocales": [
						"de",
						"it",
						"fr",
						"en"
					],
					"fallbackLocale": "de"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {
					"synchronizationMode": "None",
					"operationMode": "Server",
					"autoExpandSelect": true,
					"earlyRequests": true
				}
			}
		},
		"routing": {
			"routes": [
				{
					"pattern": ":?query:",
					"name": "Books",
					"target": "Books"
				}
			],
			"targets": {
				"Books": {
					"type": "Component",
					"id": "Books",
					"name": "sap.fe.templates.ListReport",
					"options": {
						"settings": {
							"entitySet": "Books",
							"variantManagement": "Page",
							"initialLoad": true,
							"controlConfiguration": {
								"@com.sap.vocabularies.UI.v1.SelectionFields": {
									"showClearButton": true
								}
							}
						}
					},
					"contextPattern": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.cloud": {
		"public": true,
		"service": "ch.amag.retail.dwb"
	}
}